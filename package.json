{
  "name": "draft-js-medium",
  "description": "Medium editor using Draft JS",
  "maintainers": [
    {
      "name": "Rob Gietema",
      "email": "rob.gietema@gmail.com"
    }
  ],
  "licenses": [
    {
      "type": "BSD",
      "url": "http://opensource.org/licenses/BSD-2-Clause"
    }
  ],
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:kitconcept/draft-js-medium.git"
  },
  "bugs": {
    "url": "https://github.com/kitconcept/draft-js-medium/issues",
    "email": "rob.gietema@gmail.com"
  },
  "homepage": "https://github.com/kitconcept/draft-js-medium",
  "keywords": [
    "draftjs",
    "medium"
  ],
  "main": "src/main.js",
  "scripts": {
    "start": "node server.js",
    "build": "babel src --ignore \"_*\" --out-dir lib",
    "test": "npm run test:unit",
    "test:unit": "jest --no-cache --runInBand",
    "prepublish": "npm run build",
    "watch": "npm run build -- --watch"
  },
  "jest": {
    "testRegex": "/src/.*\\.(test)\\.(js|jsx)$",
    "transform": {
      "^.+\\.js(x)?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    }
  },
  "dependencies": {
    "draft-js": "0.10.1"
  },
  "peerDependencies": {
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "2.8.2",
    "jest": "20.0.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-test-renderer": "15.6.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "engines": {
    "node": "6.9.1"
  }
}
